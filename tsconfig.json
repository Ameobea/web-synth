{
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "target": "es5",
    "lib": ["esnext", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noStrictGenericChecks": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./node_modules/@types/webassembly-js-api"],
    "types": [],
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "Tone": ["node_modules/tone/Tone"],
      "*": ["./node_modules/@types/*", "*"],
      "WebAssembly": ["./node_modules/@types/webassembly-js-api/*"]
    }
  },
  "include": ["./index.d.ts"],
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts",
    "deprecated",
    "backend",
    "faust-compiler",
    "engine"
  ]
}
