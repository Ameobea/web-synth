    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"src/**/*.{ts,js,tsx}\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "cypress:serve": "http-server dist -p 9000 -P http://127.0.0.1:9000? -c-1"
